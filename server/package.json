{
	"name": "devfest-01-rtc",
	"version": "1.0.50",
	"scripts": {
		"test": "bun test --coverage --concurrent",
		"lint": "bun biome check --fix",
		"build": "bun build ./src/index.ts --target=bun --production --outfile build/index.js",
		"dev": "bun run --watch src/index.ts",
		"start": "bun run build/index.js",
		"typecheck": "bunx tsc --noEmit"
	},
	"dependencies": {
		"@bogeychan/elysia-logger": "^0.1.10",
		"@elysiajs/cors": "^1.4.0",
		"elysia": "1.4.12",
		"pino-pretty": "^13.1.2"
	},
	"devDependencies": {
		"@biomejs/biome": "2.3.2",
		"@types/bun": "^1.3.1",
		"bun-types": "1.3.1"
	},
	"module": "src/index.js"
}
